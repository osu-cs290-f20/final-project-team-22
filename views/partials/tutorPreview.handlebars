<div class="tutor" data-name="{{name}}" data-year="{{year}}" data-price="{{hourlyRate}}" data-city="{{city}}" data-subject="{{subject}}">
  <div class="tutor-contents">
    <div class="tutor-img-container">
      <img src="{{imageURL}}" alt="{{name}}">
    </div>
    <div class="tutor-info-container">
      <span>Name: <a href="/tutors/{{name}}" class="tutor-title">{{name}}</a> </span>
      <hr class="line"> </hr>
      <span>Year: <span class="tutor-year">{{year}}</span></span>
      <hr class="line"> </hr>
      <span>Price: <span class="tutor-price">${{hourlyRate}}</span></span>
      <hr class="line"> </hr>
      <span>City: <span class="tutor-city"> {{city}} </span></span>
      <hr class="line"> </hr>
      <span>Subject: <span class="tutor-subject"> {{subject}} </span></span>
    </div>
  </div>
</div>
