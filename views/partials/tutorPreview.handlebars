<div class="tutor" data-name={{name}} data-year={{education}} data-price={{hourlyRate}}>
  <div class="tutor-contents">
    <div class="tutor-img-container">
      <img src="{{imageURL}}" alt="{{name}}">
    </div>
    <div class="tutor-info-container">
      <a href="/tutors/{{name}}" class="tutor-title">{{name}}</a> <span class="tutor-year">{{education}}</span> <span class="tutor-price">${{hourlyRate}}</span>
    </div>
  </div>
